<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	<bean id="serviceA" class="com.example.yq.ioc.service.SetterBean$ServiceA"/>
	<bean id="serviceB" class="com.example.yq.ioc.service.SetterBean$ServiceB" autowire-candidate="false"/>

	<bean id="setterBean" class="com.example.yq.ioc.service.SetterBean" autowire="byName" >
		<meta key="name" value="smallWhite"/>
	</bean>
</beans>
